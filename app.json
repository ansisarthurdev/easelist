        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F7F6F0"
      },
      "package": "com.ansisarthurdev.easelist",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
